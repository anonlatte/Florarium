<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CareScheduleItem">
        <attr name="title" format="string" />
        <attr name="icon" format="reference" />
        <attr name="scheduleValue" format="string" />
        <attr name="scheduleItemType" format="integer" />
    </declare-styleable>

    <declare-styleable name="BottomSheetItem">
        <attr name="title" />
        <attr name="icon" />
        <attr name="item_type" format="enum">
            <enum name="item_type_default" value="0" />
            <enum name="item_type_winter" value="1" />
            <enum name="item_type_last_care" value="2" />
        </attr>
    </declare-styleable>
</resources>